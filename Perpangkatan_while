#include <iostream>

using namespace std;

int main()
{
    int i, angka, pangkat, jumlah;
    i=1;
    jumlah=1;

    cout << "Masukkan angka     :";
    cin >> angka;
    cout << "Masukkan pangkat   :";
    cin >> pangkat;

    while (i <= pangkat)
    {
        jumlah=jumlah*angka;
        i++;
    }
    cout << endl << "Hasil  " << jumlah << endl;

    return 0;
}
